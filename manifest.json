{
  "manifest_version": 2,
  "name": "Rare tijden",
  "description": "Plakt de zin 'Het zijn rare tijden.' voor artikelen op de grootste Nederlandse nieuwssites.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "*://*.nu.nl/*",
        "*://*.nos.nl/*",
        "*://*.nrc.nl/*",
        "*://*.telegraaf.nl/*",
        "*://*.ad.nl/*",
        "*://*.rtlnieuws.nl/*",
        "*://*.trouw.nl/*",
        "*://*.parool.nl/*",
        "*://*.metronieuws.nl/*",
        "*://*.linda.nl/*",
        "*://pointer.kro-ncrv.nl/*",
        "*://*.volkskrant.nl/*"
        "*://omroepgelderland.nl/nieuws/*"
      ],
      "js": ["app.js"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  }
}
