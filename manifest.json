{
  "manifest_version": 2,
  "name": "Rare tijden",
  "description": "Plakt de zin 'Het zijn rare tijden.' voor artikelen op de grootste Nederlandse nieuwssites.",
  "version": "1.3",
  "content_scripts": [
    {
      "matches": [
        "*://*.ad.nl/*",
        "*://*.bd.nl/*",
        "*://*.bndestem.nl/*",
        "*://*.destentor.nl/*",
        "*://*.ed.nl/*",
        "*://*.gelderlander.nl/*",
        "*://*.linda.nl/*",
        "*://*.metronieuws.nl/*",
        "*://*.nos.nl/*",
        "*://*.nrc.nl/*",
        "*://*.nu.nl/*",
        "*://*.parool.nl/*",
        "*://*.pzc.nl/*",
        "*://*.rtlnieuws.nl/*",
        "*://*.telegraaf.nl/*",
        "*://*.tubantia.nl/*",
        "*://*.trouw.nl/*",
        "*://*.volkskrant.nl/*",
        "*://pointer.kro-ncrv.nl/*",
        "*://*.standaard.be/*",
        "*://*.demorgen.be/*",
        "*://*.tijd.be/*",
        "*://*.hln.be/*",
        "*://*.vrt.be/*",
        "*://*.nieuwsblad.be/*"
      ],
      "js": [
        "data.js",
        "app.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  }
}